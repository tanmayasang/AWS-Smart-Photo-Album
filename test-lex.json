{
  "metadata": {
    "schemaVersion": "1.0",
    "importType": "LEX",
    "importFormat": "JSON"
  },
  "resource": {
    "name": "SmartPhotoAlbumBot",
    "version": "1",
    "intents": [
      {
        "name": "SearchIntent",
        "version": "4",
        "fulfillmentActivity": {
          "type": "ReturnIntent"
        },
        "sampleUtterances": [
          "Show me {Animal} and {Food}",
          "Show me {Animal} and {Character}",
          "Show me {Food} and {Character}",
          "Show me {Food} and {Animal}",
          "Show me {Character} and {Food}",
          "Show me {Character} and {Animal}",
          "Show me {Character}",
          "Show me {Food}",
          "Show me {Animal}",
          "Show me pictures with {Animal} and {Food} in it"
        ],
        "slots": [
          {
            "sampleUtterances": [],
            "slotType": "AMAZON.Animal",
            "slotConstraint": "Optional",
            "valueElicitationPrompt": {
              "messages": [
                {
                  "contentType": "PlainText",
                  "content": "What animal?"
                }
              ],
              "maxAttempts": 2
            },
            "priority": 1,
            "name": "Animal"
          },
          {
            "sampleUtterances": [],
            "slotType": "AMAZON.FictionalCharacter",
            "slotConstraint": "Optional",
            "valueElicitationPrompt": {
              "messages": [
                {
                  "contentType": "PlainText",
                  "content": "What character?"
                }
              ],
              "maxAttempts": 2
            },
            "priority": 3,
            "name": "Character"
          },
          {
            "sampleUtterances": [],
            "slotType": "AMAZON.Food",
            "slotConstraint": "Optional",
            "valueElicitationPrompt": {
              "messages": [
                {
                  "contentType": "PlainText",
                  "content": "What food?"
                }
              ],
              "maxAttempts": 2
            },
            "priority": 2,
            "name": "Food"
          }
        ]
      }
    ],
    "voiceId": "Salli",
    "childDirected": false,
    "locale": "en-US",
    "idleSessionTTLInSeconds": 300,
    "clarificationPrompt": {
      "messages": [
        {
          "contentType": "PlainText",
          "content": "Sorry, can you please repeat that?"
        }
      ],
      "maxAttempts": 5
    },
    "abortStatement": {
      "messages": [
        {
          "contentType": "PlainText",
          "content": "Sorry, I could not understand. Goodbye."
        }
      ]
    }
  }
}